{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build:watch": {
      "cache": false,
      "dependsOn": [
        "^build:watch"
      ]
    },
    "test": {},
    "test:typecheck": {
      "cache": false
    },
    "test:watch": {
      "cache": false
    }
  }
}